function createPhoneNumber(numbers) {

    var phoneNumber = '';

    var a1 = numbers[0];
    var a2 = numbers[1];
    var a3 = numbers[2];
    var a4 = numbers[3];
    var a5 = numbers[4];
    var a6 = numbers[5];
    var a7 = numbers[6];
    var a8 = numbers[7];
    var a9 = numbers[8];
    var a10 = numbers[9];

    phoneNumber = phoneNumber.concat('(', a1, a2, a3, ')', ' ', a4, a5, a6, '-', a7,a8,a9,a10);


    return phoneNumber;


}
